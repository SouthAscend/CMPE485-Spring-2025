# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/Assignment*/.utmp/
/Assignment*/[Ll]ibrary/
/Assignment*/[Tt]emp/
/Assignment*/[Oo]bj/
/Assignment*/[Bb]uild/
/Assignment*/[Bb]uilds/
/Assignment*/[Ll]ogs/
/Assignment*/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/Assignment*/[Mm]emoryCaptures/

# Recordings can get excessive in size
/Assignment*/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/Assignment*/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/Assignment*/.vs/

# Gradle cache directory
/Assignment*/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/Assignment*/ExportedObj/
/Assignment*/.consulo/
/Assignment*/*.csproj
/Assignment*/*.unityproj
/Assignment*/*.sln
/Assignment*/*.suo
/Assignment*/*.tmp
/Assignment*/*.user
/Assignment*/*.userprefs
/Assignment*/*.pidb
/Assignment*/*.booproj
/Assignment*/*.svd
/Assignment*/*.pdb
/Assignment*/*.mdb
/Assignment*/*.opendb
/Assignment*/*.VC.db

# Unity3D generated meta files
/Assignment*/*.pidb.meta
/Assignment*/*.pdb.meta
/Assignment*/*.mdb.meta

# Unity3D generated file on crash reports
/Assignment*/sysinfo.txt

# Builds
/Assignment*/*.apk
/Assignment*/*.aab
/Assignment*/*.unitypackage
/Assignment*/*.unitypackage.meta
/Assignment*/*.app

# Crashlytics generated file
/Assignment*/crashlytics-build.properties

# Packed Addressables
/Assignment*/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/Assignment*/[Aa]ssets/[Ss]treamingAssets/aa.meta
/Assignment*/[Aa]ssets/[Ss]treamingAssets/aa/*
